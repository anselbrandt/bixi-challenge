{
  "name": "bixi-challenge",
  "version": "1.0.0",
  "engines": {
    "node": "15.x"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "postinstall": "tsc && cd web && yarn",
    "prestart": "yarn build && cd web && yarn build",
    "build": "yarn clean && tsc && yarn copy-files",
    "clean": "rm -rf ./dist",
    "copy-files": "cp ./src/public/*.* ./dist/",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "papaparse": "^5.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.28",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.5"
  }
}
